# Multi-tenant app secret (blocks random callers)
APP_TOKEN=CHANGE_THIS_TO_A_LONG_RANDOM_STRING

PORT=8080

# Point to JobNimbus Public API base (verify in your account/docs)
JOBNIMBUS_API_BASE=https://app.jobnimbus.com/api1
JOBNIMBUS_API_KEY=jn_api_key_here

# Defaults you can tune per your workflows (often required in JN)
JN_DEFAULT_CONTACT_TYPE=Residential
JN_DEFAULT_CONTACT_STATUS=New Lead
JN_DEFAULT_JOB_TYPE=Roof Replacement
JN_DEFAULT_JOB_STATUS=New

# Optional: set to "name" or "displayName" if your org requires uniqueness handling
JN_CONTACT_DISPLAY_NAME_MODE=displayName
